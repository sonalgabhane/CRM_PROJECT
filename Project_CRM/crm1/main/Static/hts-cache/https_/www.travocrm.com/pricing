		<script>
			// var sortname;
			function recordData (data) {
					 sortname = data.country;
					//  console.log(data.ip, data.country);
				}
			 $(document).ready(function() {
				
			 // get country data from database with autocomplete	event
		// 	 $( "#country" ).autocomplete({
        //     source: function( request, response ) {
        //         $.ajax( {
        //             url: "https://travocrm.com/index/getcountries",
        //             dataType:"JSON",
        //             data: {
        //                 term: request.term
        //             },
        //             success: function( data ) {
        //                 response(data);
        //             }
        //         } );
        //     },
        //     minLength: 2,
        //     select: function( event, ui ) {
        //         $("#country_id").val(ui.item.id)
        //         $("#country_name").val(ui.item.label)              
        //     }
			
        // });
		// $( "#country" ).change({
			getcountries();
		function getcountries(){
			var selectedid;
			$.ajax( {
                    url: "https://travocrm.com/pricing/getcountries",
                    dataType:"JSON",                    
                    success: function( data ) {
						var toAppend = '';
						$.each(data,function(i,o){
						toAppend += '<option value='+o.id+'>'+o.country_name+'</option>';						
						if(o.sortname == sortname)
							 selectedid = o.id;
						});

						$('#country').append(toAppend);
						$('#country').val(selectedid).trigger("change");;
						
                    }
                } );
		// });
			}
			$('#country').on('change', function() {			
				$.ajax( {
						url: "https://travocrm.com/pricing/getpricing",
						type: "POST",
						dataType:"JSON",    
						data: {id :$('#country').val()},                
						success: function( data ) {
							console.log(data);
							if(data){
							$("#oneYear").html(data.currency_name+' '+data.one_year_price);
							$("#extraUser").html(data.currency_name+' '+data.extra_user_price);
							}else{
								alert("Pricing Not Available");
							}
							
						}
					} );
			});
			
		
	});
			</script>
