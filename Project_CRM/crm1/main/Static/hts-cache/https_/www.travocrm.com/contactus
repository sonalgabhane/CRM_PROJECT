<script>
    $(document).ready(function() {
        // get city data from database with autocomplete event
        $( "#city" ).autocomplete({
            source: function( request, response ) {
                $.ajax( {
                    url: "https://travocrm.com/index/getcities",
                    dataType:"JSON",
                    data: {
                        term: request.term,
                        countryid: $("#country_id").val()
                    },
                    success: function( data ) {
                        response( data );
                    }
                } );
            },
            minLength: 2,
            select: function( event, ui ) {
                $("#city_id").val(ui.item.id)
                $("#city_name").val(ui.item.label)
            }
        });

        // get country data from database with autocomplete	event
        $( "#country" ).autocomplete({
            source: function( request, response ) {
                $.ajax( {
                    url: "https://travocrm.com/index/getcountries",
                    dataType:"JSON",
                    data: {
                        term: request.term
                    },
                    success: function( data ) {
                        // console.log(data);
                        response(data);
                    }
                } );
            },
            minLength: 2,
            select: function( event, ui ) {
                // log( "Selected: " + " aka " + ui.item.id );
                $("#country_id").val(ui.item.id)
                $("#country_name").val(ui.item.label)
                $("#phonecode").val(ui.item.phonecode);
                // $(this).next("value").val(ui.item.id);
            }
			
        });
        // to insert form data to database
        $("#formId").on("submit",function(e){
            e.preventDefault();
            $.ajax({
                'url':'https://travocrm.com/index/insertform',
                'type' : 'POST',
                'data' :  new FormData(this),
                'dataType' : 'json',
                'processData': false,
                'contentType': false,
                'success' : function(result){
                    // console.log(result);
                    // console.log(result.status);
                    if(result.status == "success"){
                        alert(result.message);
                        $("#formId").trigger('reset');
                    }else{
                        alert(result.message);
                    }

                }
            });

        });
    });
</script>

